{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "grpc/proto/**/*.proto",
        "outDir": "dist"
      }
    ],
    "watchAssets": true
  },
  "entryFile": "main"
}
